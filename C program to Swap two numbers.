#include<stdio.h>
int main() {
    double first, second, temp;
    printf("Enter the first number: ");
    scanf("%lf",&first);
    printf("Enter the second number: ");
    scanf("%lf",&second);

    temp=first;

    first=second;

    second=temp;

    printf("After swapping the first: %.2lf \n",first);
    printf("After swapping the second: %.2lf \n",second);
    return 0;

}
