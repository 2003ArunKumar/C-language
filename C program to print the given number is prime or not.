// Online C compiler to run C program online
#include<stdio.h>
int main () {
    int n,i,count=0;
    printf("Enter the positive integer: ");
    scanf("%d",&n);

    for (i=1; i<=n;i++)
 if (n%i == 0) {
    count++;
 }

 if(count==2){ 
    printf("Entered number is a prime number",n);
    }
    else {
        printf("Enterd number is not a prime number",n);
    }
    return 0;
}
